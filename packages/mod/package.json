{
    "name": "mod",
    "version": "1.0.5",
    "scripts": {
        "type-check": "tsc --noEmit --allowJs --checkJs -p jsconfig.json",
        "build": "rollup -c rollup.config.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/emdsa2/-mod"
    },
    "rollupSetting": {
        "output": "mod.js",
        "input": "src/main.js",
        "assets": {
            "location": "resources",
            "assets": [
                "Assets",
                "Audio",
                "Screens"
            ]
        },
        "componentDir": "src/components",
        "loaderName": "BC_Cloth2.user.js",
        "beta": {
            "output": "mod-beta.js",
            "loaderName": "BC_Cloth2-beta.user.js"
        }
    },
    "displayName": "服装拓展",
    "modFullName": "Echo的服装拓展",
    "author": "Echo",
    "description": "BC视觉表现的美学修正与拓展",
    "devDependencies": {
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-commonjs": "^25.0.8",
        "@rollup/plugin-node-resolve": "^15.2.4",
        "@rollup/plugin-replace": "^5.0.7",
        "@rollup/plugin-terser": "^0.4.4",
        "bc-stubs": "^111.0.0",
        "bondage-club-mod-sdk": "^1.2.0",
        "rollup": "^4.22.4",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-import-css": "^3.5.7"
    }
}
